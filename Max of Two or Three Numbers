#include <iostream>
using namespace std;

int max(int a, int b) {
    return (a > b) ? a : b;
}

int max(int a, int b, int c) {
    return max(max(a, b), c);
}

double max(double a, double b) {
    return (a > b) ? a : b;
}

int main() {
    cout << "Max of 5 and 8: " << max(5, 8) << endl;
    cout << "Max of 3.2 and 2.5: " << max(3.2, 2.5) << endl;
    cout << "Max of 3, 6, 1: " << max(3, 6, 1) << endl;
    return 0;
}
